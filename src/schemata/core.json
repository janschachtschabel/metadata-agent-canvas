          "type": "closed",
          "concepts": [
            { "label": "CC0" },
            { "label": "CC BY" },
            { "label": "CC BY-SA" },
            { "label": "CC BY-ND" },
            { "label": "CC BY-NC" },
            { "label": "CC BY-NC-SA" },
            { "label": "CC BY-NC-ND" }
          ]
        },
        "normalization": { "trim": true, "case": "upper" }
