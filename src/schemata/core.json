      "system": {
        "path": "ccm:commonlicense_cc_version",
        "uri": "ccm:commonlicense_cc_version",
        "datatype": "string",
        "multiple": false,
        "required": false,
        "ask_user": false,
        "ai_fillable": true,
        "index": { "keyword": true },
        "validation": { "pattern": "^[0-9]+(\\.[0-9]+)?$" },
        "normalization": { "trim": true }
      }
    },
    {
      "id": "cclom:general_language",
      "group": "classification",
      "group_label": "Klassifikation",
      "prompt": {
        "label": "Sprache",
        "description": "Sprachcode nach BCP-47 (z. B. de, en, de-DE)."
