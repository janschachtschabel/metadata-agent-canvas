    {
      "id": "cclom:title",
      "group": "description",
      "group_label": "Beschreibung",
      "prompt": {
        "label": "Titel",
        "description": "Aussagekräftiger, eigenständiger Titel der Ressource.",
        "examples": ["Zukunft der Hochschullehre"]
      },
      "system": {
        "path": "cclom:title",
        "uri": "cclom:title",
        "datatype": "string",
        "multiple": false,
        "required": true,
        "ask_user": true,
        "ai_fillable": true,
        "index": { "fulltext": true, "keyword": true },
        "normalization": { "trim": true, "collapseWhitespace": true }
      }
    },
    {
      "id": "cclom:general_description",
      "group": "description",
      "group_label": "Beschreibung",
      "prompt": {
        "label": "Beschreibungstext",
        "description": "Kurzbeschreibung oder Abstract der Ressource.",
        "minLength": 40
      },
      "system": {
        "path": "cclom:general_description",
        "uri": "cclom:general_description",
        "datatype": "string",
        "multiple": false,
        "required": true,
        "ask_user": true,
        "ai_fillable": true,
        "index": { "fulltext": true },
        "normalization": { "trim": true, "collapseWhitespace": true }
      }
    },
    {
      "id": "cclom:general_keyword",
      "group": "description",
      "group_label": "Beschreibung",
      "prompt": {
        "label": "Keywords",
        "description": "Schlagwörter/Themen (Kommagetrennt oder Liste).",
        "examples": ["KI, Hochschuldidaktik", ["Künstliche Intelligenz", "Digitale Lehre"]]
      },
      "system": {
        "path": "cclom:general_keyword",
        "uri": "cclom:general_keyword",
        "datatype": "array",
        "multiple": true,
        "required": true,
        "ask_user": true,
        "ai_fillable": true,
        "index": { "keyword": true },
        "normalization": { "trim": true, "deduplicate": true, "collapseWhitespace": true }
      }
    },
    {
      "id": "ccm:wwwurl",
      "group": "publication",
      "group_label": "Publikation",
      "prompt": {
        "label": "Web-URL",
        "description": "Öffentliche URL, unter der die Ressource erreichbar ist.",
        "examples": ["https://www.beispiel.de/angebot"]
      },
      "system": {
        "path": "ccm:wwwurl",
        "uri": "ccm:wwwurl",
        "datatype": "uri",
        "multiple": false,
        "required": false,
        "ask_user": true,
        "ai_fillable": true,
        "index": { "keyword": true },
        "validation": { "pattern": "^(https?://[^\\s]+)$" },
        "normalization": { "trim": true }
      }
    },
    {
      "id": "preview:url",
      "group": "publication",
      "group_label": "Publikation",
      "prompt": {
        "label": "URL des Vorschaubildes",
        "description": "Direkter Link zu einem Vorschaubild/Thumbnail.",
        "examples": ["https://cdn.beispiel.de/thumbs/123.jpg"]
      },
      "system": {
        "path": "preview:url",
        "uri": "preview:url",
        "datatype": "uri",
        "multiple": false,
        "required": false,
        "ask_user": false,
        "ai_fillable": true,
        "index": { "keyword": true },
        "validation": { "pattern": "^(https?://[^\\s]+\\.(jpg|jpeg|png|webp))$" },
        "normalization": { "trim": true }
      }
    },
    {
      "id": "ccm:author_freetext",
