      "system": {
        "path": "cclom:general_language",
        "uri": "cclom:general_language",
        "datatype": "string",
        "multiple": false,
        "required": false,
        "ask_user": true,
        "ai_fillable": true,
        "index": { "keyword": true },
        "validation": { "pattern": "^[a-z]{2}(-[A-Z]{2})?$" },
        "normalization": { "trim": true, "lowercase_lang": true }
      }
    },
