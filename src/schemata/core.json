    },
    {
      "id": "ccm:commonlicense_cc_version",
      "group": "license",
      "group_label": "Lizenz",
      "prompt": {
        "label": "Lizenzversion",
        "description": "Versionsnummer der CC-Lizenz (z. B. 4.0)."
