{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist/webkomponente-canvas/browser",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|svg|ico|json|woff|woff2|ttf|eot))",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
